<Page x:Class="RemotePC8801.PageDiskInfo"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:RemotePC8801"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="PageDiskInfo"
      Loaded="Page_Loaded">

    <Grid>
        <Button x:Name="ButtonReload" Click="ButtonReload_Click" HorizontalAlignment="Left" VerticalAlignment="Top">RELOAD</Button>
        <ListBox x:Name="ListBoxDiskInfos" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="0,20,0,0" >
        </ListBox>
    </Grid>
</Page>
